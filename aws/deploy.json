{
    "jobDefinitionName": "staging-deploy",
    "type": "container",
    "parameters": {},
    "containerProperties": {
        "image": "398048034572.dkr.ecr.us-east-1.amazonaws.com/reconfigureio/build-framework/f1-staging-deploy:latest",
        "vcpus": 8,
        "memory": 119678,
        "jobRoleArn": "arn:aws:iam::398048034572:role/deployment-worker",
        "command": [
            "/run.sh"
        ],
        "volumes": [
            {
                "host": {
                    "sourcePath": "/opt/Xilinx"
                },
                "name": "xilinx"
            },
            {
                "host": {
                    "sourcePath": "/dev"
                },
                "name": "dev"
            }
        ],
        "environment": [
            {
                "name": "XILINX_SDX",
                "value": "/opt/Xilinx/SDx/2018.2.op"
            }
        ],
        "mountPoints": [
            {
                "sourceVolume": "xilinx",
                "containerPath": "/opt/Xilinx"
            },
            {
                "sourceVolume": "dev",
                "containerPath": "/dev"
            }
        ],
        "readonlyRootFilesystem": false,
        "privileged": true,
        "ulimits": [],
        "user": "root"
    }
}
